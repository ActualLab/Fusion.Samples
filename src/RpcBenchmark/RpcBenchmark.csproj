<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <AssemblyName>Samples.RpcBenchmark</AssemblyName>
    <RootNamespace>Samples.RpcBenchmark</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="7.0.9" />
    <PackageReference Include="Stl.Fusion" Version="6.1.110" />
    <PackageReference Include="Stl.Rpc.Server" Version="6.1.110" />
    <PackageReference Include="Stl.RestEase" Version="6.1.110" />
  </ItemGroup>

  <!-- Roslyn generators/analyzers -->
  <ItemGroup>
    <PackageReference Include="MemoryPack.Generator" Version="1.9.16" />
    <PackageReference Include="RestEase.SourceGenerator" Version="1.6.4" />
    <PackageReference Include="Stl.Generators" Version="6.1.110" />
  </ItemGroup>

</Project>
